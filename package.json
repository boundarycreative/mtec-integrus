{
  "name": "mtec-intergrus",
  "version": "1.0.0",
  "description": "M-Tec Intergrus Scss",
  "scripts": {
   "sass": "sass --watch --poll src/scss/app.scss:public/css/app.css --load-path=src/scss",
    "serve": "browser-sync start --server \"public\" --files \"public/**/*\"",
    "dev": "npm-run-all --parallel sass serve"
  },
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.75.0"
  }
}
